{
	"llvm-target":   "wasm32-unknown-wasi",
	"cpu":           "generic",
	"features":      "",
	"build-tags":    ["tinygo.wasm", "wasi"],
	"goos":          "linux",
	"goarch":        "arm",
	"linker":        "wasm-ld",
	"libc":          "wasi-libc",
	"scheduler":     "asyncify",
	"default-stack-size": 16384,
	"cflags": [
		"-mno-bulk-memory"
	],
	"ldflags": [
		"--stack-first",
		"--no-demangle"
	],
	"extra-files": [
		"src/runtime/asm_tinygowasm.S"
	],
	"emulator":      "wasmtime --mapdir=/tmp::{tmpDir} {}",
	"wasm-abi":      "generic"
}
